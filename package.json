    "react-dom": "^15.4.2",
    "webpack": "^3.8.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "emotion": "^9.1.1",
    "prop-types": "^15.5.10",
    "react-emotion": "^9.1.1",
    "recompose": "0.26.0"
